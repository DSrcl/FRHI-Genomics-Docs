        <valueString value="The player of the role is the wife of scoping person&#39;s father and not the scoping person&#39;s natural mother."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="STPMTH"/>
      <display value="stepmother"/>
    </contains>
    <contains>
