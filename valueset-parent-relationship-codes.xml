    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player of the role is one who begets, gives birth to, or nurtures and raises the scoping entity (child)."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="PRN"/>
      <display value="parent"/>
    </contains>
    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player of the role (parent) has taken the scoper (child) into their family through legal means and raises them as his or her own child."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="ADOPTP"/>
      <display value="adoptive parent"/>
    </contains>
    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player of the role (father) is a male who has taken the scoper (child) into their family through legal means and raises them as his own child."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="ADOPTF"/>
      <display value="adoptive father"/>
    </contains>
    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player of the role (father) is a female who has taken the scoper (child) into their family through legal means and raises them as her own child."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="ADOPTM"/>
      <display value="adoptive mother"/>
    </contains>
