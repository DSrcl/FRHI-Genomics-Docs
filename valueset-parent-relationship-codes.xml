      <code value="NFTHF"/>
      <display value="natural father of fetus"/>
    </contains>
    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player of the role is the husband of scoping person&#39;s mother and not the scoping person&#39;s natural father."/>
