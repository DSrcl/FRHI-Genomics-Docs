      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="STPFTH"/>
      <display value="stepfather"/>
    </contains>
    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player of the role is a female who conceives, gives birth to, or raises and nurtures the scoping entity (child)."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="MTH"/>
      <display value="mother"/>
    </contains>
    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player is a female whose womb carries the fetus of the scoper.  Generally used when the gestational mother and natural mother are not the same."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="GESTM"/>
