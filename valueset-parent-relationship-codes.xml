      <code value="NMTH"/>
      <display value="natural mother"/>
    </contains>
    <contains>
      <extension url="http://hl7.org/fhir/StructureDefinition/valueset-definition">
        <valueString value="The player is the biologic female parent of the scoping fetus."/>
      </extension>
      <system value="http://hl7.org/fhir/v3/RoleCode"/>
      <code value="NMTHF"/>
