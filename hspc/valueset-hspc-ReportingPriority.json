    "contains": [
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "As soon as possible, next highest priority after stat."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "A",
        "display": "ASAP"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Filler should contact the placer as soon as results are available, even for preliminary results.  (Was \"C\" in HL7 verion 2.3's reporting priority.)"
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "CR",
        "display": "callback results"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Filler should contact the placer (or target) to schedule the service.  (Was \"C\" in HL7 version 2.3's TQ-priority component.)"
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "CS",
        "display": "callback for scheduling"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Filler should contact the placer to schedule the service.  (Was \"C\" in HL7 version 2.3's TQ-priority component.)"
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "CSP",
        "display": "callback placer for scheduling"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Filler should contact the service recipient (target) to schedule the service.  (Was \"C\" in HL7 version 2.3's TQ-priority component.)"
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "CSR",
        "display": "contact recipient for scheduling"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Beneficial to the patient but not essential for survival."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "EL",
        "display": "elective"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "An unforeseen combination of circumstances or the resulting state that calls for immediate action."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "EM",
        "display": "emergency"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Used to indicate that a service is to be performed prior to a scheduled surgery.  When ordering a service and using the pre-op priority, a check is done to see the amount of time that must be allowed for performance of the service.  When the order is placed, a message can be generated indicating the time needed for the service so that it is not ordered in conflict with a scheduled operation."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "P",
        "display": "preop"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "An \"as needed\" order should be accompanied by a description of what constitutes a need. This description is represented by an observation service predicate as a precondition."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "PRN",
        "display": "as needed"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Routine service, do at usual work hours."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "R",
        "display": "routine"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "A report should be prepared and sent as quickly as possible."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "RR",
        "display": "rush reporting"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "With highest priority (e.g., emergency)."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "S",
        "display": "stat"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "It is critical to come as close as possible to the requested time (e.g., for a through antimicrobial level)."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "T",
        "display": "timing critical"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Drug is to be used as directed by the prescriber."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "UD",
        "display": "use as directed"
      },
      {
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/valueset-definition",
            "valueString": "Calls for prompt action."
          }
        ],
        "system": "http://hl7.org/fhir/v3/ActPriority",
        "code": "UR",
        "display": "urgent"
      }
    ]
  }
